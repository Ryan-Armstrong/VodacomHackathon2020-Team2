.card-tf {
  border: 1px solid;
  border-color: #c5c5c5;
  border-radius: 8px;
  width: 90vw;
  height: 10vh;
  margin-top: 2vh;
  position: relative;
  background-color: #fcfcfc;
}

.text-primary-tf {
  text-align: left;
  margin: 2vh;
  width: 80%;
}

.text-arrow-tf {
  text-align: right;
  margin-right: 2vh;
  margin-top: 3vh;
  width: 20%;
}
