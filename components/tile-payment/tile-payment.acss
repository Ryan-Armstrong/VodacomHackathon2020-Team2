.card-tp {
  border: 1px solid;
  border-color: #c5c5c5;
  border-radius: 8px;
  width: 26vw;
  height: 14vh;
  margin-top: 2vh;
  position: relative;
  background-color: #fcfcfc;
}

.text-primary-tp {
  position: absolute;
  display: block;
  top: 10vh;
  width: 100%;
  text-align: center;
}

.image-payment-tp {
  height: 8vh;
  width: 12vw;
  top: 2vh;
  position: absolute;
  display: block;
  left: 7vw;
}
